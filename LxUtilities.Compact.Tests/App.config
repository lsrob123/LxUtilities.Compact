<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="redisCacheClient" type="LxUtilities.Compact.Cache.Redis.Config.ConfigSectionHandler, LxUtilities.Compact"/>
  </configSections>

  <redisCacheClient allowAdmin="true" ssl="false" connectTimeout="5000" database="0">
    <hosts>
      <add host="127.0.0.1" cachePort="6379"/>
    </hosts>
  </redisCacheClient>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
